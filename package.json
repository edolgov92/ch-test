{
  "name": "proxy",
  "version": "0.0.1",
  "description": "Proxy API Service to connect Source and Target aplications",
  "author": "Eugene Dolgov",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "npm run test:unit && npm run test:e2e",
    "test:unit": "jest --config ./test/unit/jest-unit.json",
    "test:unit:watch": "jest --config ./test/unit/jest-unit.json --watch",
    "test:e2e": "jest --config ./test/e2e/jest-e2e.json",
    "test:e2e:watch": "jest --config ./test/e2e/jest-e2e.json --watch"
  },
  "dependencies": {
    "@golevelup/nestjs-graphql-request": "0.1.16",
    "@nestjs/axios": "3.0.0",
    "@nestjs/common": "10.2.2",
    "@nestjs/config": "3.0.1",
    "@nestjs/core": "10.2.2",
    "@nestjs/jwt": "10.1.1",
    "@nestjs/microservices": "10.2.2",
    "@nestjs/passport": "10.0.1",
    "@nestjs/platform-express": "10.2.2",
    "@nestjs/swagger": "7.1.10",
    "async-retry": "1.3.3",
    "axios": "1.5.0",
    "bcrypt": "5.1.1",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.0",
    "dotenv": "16.3.1",
    "express-rate-limit": "6.9.0",
    "graphql-request": "6.1.0",
    "helmet": "7.0.0",
    "kafkajs": "2.2.4",
    "limiter": "2.1.0",
    "passport-jwt": "4.0.1",
    "rand-token": "1.0.1",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.8.1",
    "short-uuid": "4.2.2"
  },
  "devDependencies": {
    "@nestjs/cli": "10.1.16",
    "@nestjs/schematics": "10.0.2",
    "@nestjs/testing": "10.2.2",
    "@types/async-retry": "1.4.5",
    "@types/bcrypt": "5.0.0",
    "@types/express": "4.17.17",
    "@types/jest": "29.5.4",
    "@types/passport-jwt": "3.0.9",
    "@types/supertest": "2.0.12",
    "@typescript-eslint/eslint-plugin": "6.5.0",
    "@typescript-eslint/parser": "6.5.0",
    "eslint": "8.48.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-prettier": "5.0.0",
    "jest": "29.6.4",
    "prettier": "3.0.3",
    "supertest": "6.3.3",
    "ts-jest": "29.1.1",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.2.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
